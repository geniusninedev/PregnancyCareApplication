<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_pre_pregnancy"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="com.nineinfosys.android.pregnancycareapplication.home.PrePregnancy"
    android:background="@drawable/simple">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:id="@+id/textViewpre"
        android:textSize="10pt"
        android:paddingLeft="5dp"
        android:gravity="center"
        android:textColor="#000000"

        android:fontFamily="serif"
        />


    <ListView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/listViewPregnancy"
        android:divider="@drawable/divider"
        android:dividerHeight="30dp"
        android:layout_marginTop="40dp"
        android:paddingLeft="20dp"
        />
</LinearLayout>
